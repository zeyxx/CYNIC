{
  "name": "@cynic/node",
  "version": "0.1.0",
  "description": "CYNIC Node - Decentralized Collective Consciousness Node",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "cynic": "./bin/cynic.js"
  },
  "exports": {
    ".": "./src/index.js",
