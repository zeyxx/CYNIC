    "./services/event-bus-bridge.js": "./src/services/event-bus-bridge.js",
    "./services/memory-coordinator.js": "./src/services/memory-coordinator.js",
    "./chaos": "./src/chaos/index.js",
    "./inference": "./src/inference/index.js",
    "./tracing/dog-tracing.js": "./src/tracing/dog-tracing.js",
