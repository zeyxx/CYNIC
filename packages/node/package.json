  "scripts": {
    "test": "node --test --test-concurrency=4 test/*.test.js",
    "test:seq": "node --test --test-concurrency=1 test/*.test.js",
    "test:fast": "node --test --test-concurrency=8 test/*.test.js",
    "start": "node bin/cynic.js start"
  },
  "dependencies": {
    "@cynic/anchor": "*",
    "@cynic/burns": "*",
    "@cynic/core": "*",
    "@cynic/emergence": "*",
    "@cynic/identity": "*",
    "@cynic/protocol": "*",
    "blessed": "^0.1.81",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.0",
    "commander": "^14.0.2",
    "express": "^5.2.1",
    "ws": "^8.18.0"
